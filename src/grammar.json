{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "shopware_twig",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "statement_directive"
      }
    },
    "statement_directive": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{%"
        },
        {
          "type": "SYMBOL",
          "name": "tag_statement"
        },
        {
          "type": "STRING",
          "value": "%}"
        }
      ]
    },
    "tag_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "tag"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "variable"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "tag": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "block"
        },
        {
          "type": "STRING",
          "value": "endblock"
        }
      ]
    },
    "variable": {
      "type": "STRING",
      "value": "some_block"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}