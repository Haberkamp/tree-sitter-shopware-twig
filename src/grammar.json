{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "shopware_twig",
  "rules": {
    "template": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "twig_comment"
          },
          {
            "type": "SYMBOL",
            "name": "statement_directive"
          },
          {
            "type": "SYMBOL",
            "name": "html_element"
          },
          {
            "type": "SYMBOL",
            "name": "style_element"
          },
          {
            "type": "SYMBOL",
            "name": "html_doctype"
          },
          {
            "type": "SYMBOL",
            "name": "html_entity"
          },
          {
            "type": "SYMBOL",
            "name": "content"
          },
          {
            "type": "SYMBOL",
            "name": "erroneous_end_tag"
          }
        ]
      }
    },
    "content": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "PATTERN",
        "value": "[^<>&\\{\\s]([^<>&\\{]*[^<>&\\{\\s])?"
      }
    },
    "html_element": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "html_void_tag"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "html_start_tag"
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "_node"
              }
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "html_end_tag"
                },
                {
                  "type": "SYMBOL",
                  "name": "_implicit_end_tag"
                }
              ]
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "html_self_closing_tag"
        }
      ]
    },
    "style_element": {
      "type": "SEQ",
      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "style_start_tag"
          },
          "named": true,
          "value": "html_start_tag"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "raw_text"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "html_end_tag"
        }
      ]
    },
    "style_start_tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "<"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "_style_start_tag_name"
          },
          "named": true,
          "value": "html_tag_name"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "html_attribute"
          }
        },
        {
          "type": "STRING",
          "value": ">"
        }
      ]
    },
    "html_void_tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "html_start_tag"
        },
        {
          "type": "SYMBOL",
          "name": "_implicit_end_tag"
        }
      ]
    },
    "_node": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "twig_comment"
        },
        {
          "type": "SYMBOL",
          "name": "html_element"
        },
        {
          "type": "SYMBOL",
          "name": "html_entity"
        },
        {
          "type": "SYMBOL",
          "name": "content"
        }
      ]
    },
    "html_start_tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "<"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "_start_tag_name"
          },
          "named": true,
          "value": "html_tag_name"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "html_attribute"
          }
        },
        {
          "type": "STRING",
          "value": ">"
        }
      ]
    },
    "html_end_tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "</"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "_end_tag_name"
          },
          "named": true,
          "value": "html_tag_name"
        },
        {
          "type": "STRING",
          "value": ">"
        }
      ]
    },
    "html_self_closing_tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "<"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "_start_tag_name"
          },
          "named": true,
          "value": "html_tag_name"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "html_attribute"
          }
        },
        {
          "type": "STRING",
          "value": "/>"
        }
      ]
    },
    "erroneous_end_tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "</"
        },
        {
          "type": "SYMBOL",
          "name": "erroneous_end_tag_name"
        },
        {
          "type": "STRING",
          "value": ">"
        }
      ]
    },
    "html_attribute": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "html_attribute_name"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "="
                },
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "SYMBOL",
                      "name": "html_attribute_value"
                    },
                    {
                      "type": "SYMBOL",
                      "name": "html_quoted_attribute_value"
                    }
                  ]
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "html_attribute_name": {
      "type": "PATTERN",
      "value": "[@:#]?[a-zA-Z][a-zA-Z0-9_\\-ðŸ’©]*"
    },
    "html_attribute_value": {
      "type": "PATTERN",
      "value": "[^>\\s\"'=]+"
    },
    "html_quoted_attribute_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "\""
            },
            {
              "type": "ALIAS",
              "content": {
                "type": "PATTERN",
                "value": "[^\"]*"
              },
              "named": true,
              "value": "html_attribute_value"
            },
            {
              "type": "STRING",
              "value": "\""
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "'"
            },
            {
              "type": "ALIAS",
              "content": {
                "type": "PATTERN",
                "value": "[^']*"
              },
              "named": true,
              "value": "html_attribute_value"
            },
            {
              "type": "STRING",
              "value": "'"
            }
          ]
        }
      ]
    },
    "html_doctype": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "<!"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "_doctype"
          },
          "named": false,
          "value": "doctype"
        },
        {
          "type": "PATTERN",
          "value": "[^>]+"
        },
        {
          "type": "STRING",
          "value": ">"
        }
      ]
    },
    "_doctype": {
      "type": "PATTERN",
      "value": "[Dd][Oo][Cc][Tt][Yy][Pp][Ee]"
    },
    "statement_directive": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{%"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "if_statement"
            },
            {
              "type": "SYMBOL",
              "name": "tag_statement"
            },
            {
              "type": "SYMBOL",
              "name": "function_call"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "%}"
        }
      ]
    },
    "if_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "conditional"
        },
        {
          "type": "SYMBOL",
          "name": "variable"
        }
      ]
    },
    "tag_statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "tag"
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "variable"
                },
                {
                  "type": "BLANK"
                }
              ]
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "conditional"
        }
      ]
    },
    "tag": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "block"
        },
        {
          "type": "STRING",
          "value": "endblock"
        }
      ]
    },
    "conditional": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "if"
        },
        {
          "type": "STRING",
          "value": "endif"
        }
      ]
    },
    "variable": {
      "type": "PATTERN",
      "value": "[a-zA-Z0-9_]+"
    },
    "function_call": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "function_identifier"
        },
        {
          "type": "SYMBOL",
          "name": "arguments"
        }
      ]
    },
    "function_identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_]*"
    },
    "arguments": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "html_entity": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PATTERN",
          "value": "&[a-zA-Z][a-zA-Z0-9]*;"
        },
        {
          "type": "PATTERN",
          "value": "&#[0-9]+;"
        },
        {
          "type": "PATTERN",
          "value": "&#[xX][0-9a-fA-F]+;"
        }
      ]
    },
    "twig_comment": {
      "type": "PATTERN",
      "value": "\\{#[^#]*(?:#[^}][^#]*)*?#\\}"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [
    [
      "html_element",
      "html_void_tag"
    ]
  ],
  "precedences": [],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "_start_tag_name"
    },
    {
      "type": "SYMBOL",
      "name": "_style_start_tag_name"
    },
    {
      "type": "SYMBOL",
      "name": "_end_tag_name"
    },
    {
      "type": "SYMBOL",
      "name": "erroneous_end_tag_name"
    },
    {
      "type": "STRING",
      "value": "/>"
    },
    {
      "type": "SYMBOL",
      "name": "_implicit_end_tag"
    },
    {
      "type": "SYMBOL",
      "name": "raw_text"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}